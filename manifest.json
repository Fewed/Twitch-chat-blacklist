{
"content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "manifest_version": 2,
  "name": "Twitch Chat Blacklist",
  "version": "1.0",
  "content_scripts": [
  {
    "matches": [
      "<all_urls>"
    ],
    "js": ["content/content.js"]
  }
],
"browser_action": {
 
    "default_popup":"popup/popup.html"
},

"background": {
  "page": "background/background.html",
  "persistent": false
},

"permissions": [
    "tabs",
    "storage"
  ]
}